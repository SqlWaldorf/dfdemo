{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "njdfv2"
		},
		"babsBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'babsBlobStorage'"
		},
		"babsSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'babsSqlDatabase'"
		},
		"SourceDataset_fnh_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "201508_trip_data.csv"
		},
		"SourceDataset_fnh_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "u2ulabs/data/babs/source"
		},
		"statusdataBlob_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "201508_status_data.csv.gz"
		},
		"statusdataBlob_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "u2ulabs/data/babs/source"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://njkeys.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"babsBlobStorage_connectionString": {
						"value": "[parameters('babsBlobStorage_connectionString')]"
					},
					"babsSqlDatabase_connectionString": {
						"value": "[parameters('babsSqlDatabase_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"SourceDataset_fnh_properties_typeProperties_fileName": {
						"value": "[parameters('SourceDataset_fnh_properties_typeProperties_fileName')]"
					},
					"SourceDataset_fnh_properties_typeProperties_folderPath": {
						"value": "[parameters('SourceDataset_fnh_properties_typeProperties_folderPath')]"
					},
					"statusdataBlob_properties_typeProperties_fileName": {
						"value": "[parameters('statusdataBlob_properties_typeProperties_fileName')]"
					},
					"statusdataBlob_properties_typeProperties_folderPath": {
						"value": "[parameters('statusdataBlob_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}